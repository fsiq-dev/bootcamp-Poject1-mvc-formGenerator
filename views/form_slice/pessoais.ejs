<fieldset class="borderFieldset"><legend>Informações Pessoais</legend>
    <div class="formGroup">
        <label for="name">Nome:</label>
        <input type="text" name="name" placeholder="Nome Sobrenome">
    </div>
    <div class="formGroup">
        <label for="email">E-mail:</label>
        <input type="email" name="email" placeholder="email@email.com">
    </div>
    <div class="formGroup">
        <label for="cpf">Cpf:</label>
        <input type="text" name="cpf" placeholder="000.000.00-00" required>
    </div>
    <div class="formGroup">
        <label for="phone">Telefone:</label>
        <input type="tel" placeholder="xxx-xxxxx-xxxx" name="phone" required>
    </div>
</fieldset>